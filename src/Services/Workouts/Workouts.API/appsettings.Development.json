{
	"Serilog": {
		"MinimumLevel": {
			"Default": "Information",
			"Override": {
				"Microsoft": "Warning",
				"System": "Warning"
			}
		}
	},
	"ConnectionStrings": {
		"DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=Workout;Trusted_Connection=True;"
	},
	"GrpcConfig": {
		"Url": "https://localhost:7173"
	},
	"ConsumerConfig": {
		"BootstrapServers": "localhost:9092", // Настройки Kafka брокера
		"GroupId": "workouts_users_group"
	},
	"ElasticConfiguration": {
		"Uri": "http://elasticsearch:9200"
	},
	"Redis": {
		"Url": "localhost:6379",
		"SlidingExpirationTimeInMinutes": 30
	}
}
