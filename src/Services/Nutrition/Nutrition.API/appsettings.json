{
	"Serilog": {
		"MinimumLevel": {
			"Default": "Information",
			"Override": {
				"Microsoft": "Warning",
				"System": "Warning"
			}
		}
	},
	"AllowedHosts": "*",
	"JwtSettings": {
		"Issuer": "IdentityServer",
		"Audience": "AuthClient",
		"SecretKey": "This is my custom Secret key for authentication",
		"ExpiryInMinutes": 60
	},
	"ElasticConfiguration": {
		"Uri": "http://elasticsearch:9200"
	}
}
